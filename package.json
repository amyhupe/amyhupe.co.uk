{
  "name": "amyhupe.co.uk",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run clean && npm run generate-dev & npm run sass-dev",
    "build": "npm run clean && npm run generate && npm run sass",
    "clean": "rm -rf dist",
    "sass": "sass --style compressed --no-source-map src/assets/scss:dist/assets/css",
    "sass-dev": "sass --watch src/assets/scss:dist/assets/css",
    "generate": "npx eleventy",
    "generate-dev": "ELEVENTY_ENV=development npx eleventy --serve"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-rss": "^1.0.5",
    "clean-css": "^4.1.11",
    "html-minifier": "^3.5.19",
    "luxon": "^1.3.1",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "marked": "^0.8.0",
    "nunjucks-markdown": "^2.0.1",
    "require-dir": "^1.0.0",
    "sass": "^1.39.0"
  },
  "dependencies": {
    "moment": "^2.22.2"
  }
}
